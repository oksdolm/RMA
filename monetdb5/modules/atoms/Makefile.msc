	$(CC) /EHsc $(CFLAGS) $(batxml_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBATOMS "-Fobatxml.obj" -c "$(srcdir)\batxml.c"
str.obj: "$(srcdir)\str.c" "$(srcdir)\str.h" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h"
	$(CC) /EHsc $(CFLAGS) $(str_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBATOMS "-Fostr.obj" -c "$(srcdir)\str.c"
mcurl.obj: "$(srcdir)\mcurl.c" "$(srcdir)\..\..\..\gdk\gdk.h" "$(srcdir)\..\..\mal\mal.h" "$(srcdir)\..\..\mal\mal_instruction.h" "$(srcdir)\..\..\mal\mal_type.h" "$(srcdir)\..\..\mal\mal_stack.h" "$(srcdir)\..\..\mal\mal_namespace.h" "$(srcdir)\..\..\mal\mal_errors.h" "$(srcdir)\..\..\mal\mal_exception.h"
	$(CC) /EHsc $(CFLAGS) $(mcurl_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBATOMS "-Fomcurl.obj" -c "$(srcdir)\mcurl.c"
