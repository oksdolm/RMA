	$(CC) /EHsc $(CFLAGS) $(identifier_CFLAGS) $(GENDLL) -D_CRT_SECURE_NO_WARNINGS -DLIBATOMS "-Foidentifier.obj" -c "$(srcdir)\identifier.c"
